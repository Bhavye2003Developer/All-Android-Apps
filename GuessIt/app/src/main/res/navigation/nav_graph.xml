<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startGameFragment">

    <fragment
        android:id="@+id/startGameFragment"
        android:name="com.example.guessit.startGameFragment"
        android:label="startGameFragment" >
        <action
            android:id="@+id/action_startGameFragment_to_guessWordFragment2"
            app:destination="@id/guessWordFragment2" />
    </fragment>
    <fragment
        android:id="@+id/gameFinishedFragment"
        android:name="com.example.guessit.GameFinishedFragment"
        android:label="GameFinishedFragment" >
        <action
            android:id="@+id/action_gameFinishedFragment_to_guessWordFragment22"
            app:destination="@id/guessWordFragment2"
            app:popUpTo="@id/startGameFragment"/>
        <action
            android:id="@+id/action_gameFinishedFragment_to_startGameFragment2"
            app:destination="@id/startGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/guessWordFragment2"
        android:name="com.example.guessit.GuessWordFragment"
        android:label="GuessWordFragment" >
        <action
            android:id="@+id/action_guessWordFragment2_to_gameFinishedFragment"
            app:destination="@id/gameFinishedFragment" />
    </fragment>
</navigation>