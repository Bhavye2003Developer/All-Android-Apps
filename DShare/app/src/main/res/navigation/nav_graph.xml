<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home2">

    <fragment
        android:id="@+id/home2"
        android:name="com.example.dshare.shareFragments.Home"
        android:label="Home" >
        <action
            android:id="@+id/action_home2_to_sender"
            app:destination="@id/sender" />
        <action
            android:id="@+id/action_home2_to_receiver"
            app:destination="@id/receiver" />
    </fragment>
    <fragment
        android:id="@+id/sender"
        android:name="com.example.dshare.shareFragments.Sender"
        android:label="Sender" />
    <fragment
        android:id="@+id/receiver"
        android:name="com.example.dshare.shareFragments.Receiver"
        android:label="Receiver" />
</navigation>